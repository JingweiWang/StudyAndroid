<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.jingweiwang.audiocontroller">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayerFrontActivity"
            android:label="@string/title_activity_player_front" />
        <activity
            android:name=".PlayerFrontActivityByBilibili"
            android:label="@string/title_activity_player_front_bilibili" />
        <activity
            android:name=".PlayerBackActitity"
            android:label="@string/title_activity_player_back" />

        <service android:name=".MusicService" />
    </application>

</manifest>